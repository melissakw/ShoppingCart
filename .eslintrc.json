{
  "env": {
      "browser": true,
      "es6": true
  },
  "parser": "babel-eslint",
  "extends": [
      "plugin:react/recommended",
      "prettier",
      "prettier/react"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "prettier"
  ],
  "rules": {
      // "prettier/prettier": "off",
      "react/prop-types": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/state-in-constructor": "off"
  }
}
